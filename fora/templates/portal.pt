<metal:main use-macro="load: layout.pt">
  <metal:style fill-slot="style">
  </metal:style>
  <div id="panel-forums" class="panel panel-default" metal:fill-slot="content">
    <!-- Default panel contents -->
    <div class="panel-heading">Forums</div>

    <div class="panel-body">
      <p>Here lies the available forums.</p>
    </div>

    <!-- List group -->
    <div id="list-forum" class="list-group"></div>

    <div class="panel-footer">Come back for more!</div>
  </div>

  <metal:extra fill-slot="extra">
    <script src="${request.static_url('fora:static/js/fora.portal.js')}"></script>
  </metal:extra>
</metal:main>
